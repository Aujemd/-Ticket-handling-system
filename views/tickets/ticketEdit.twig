{% extends "users/dashboard.twig" %}
{% block mostrar %}
	<div >
		<form action="{{url}}Update/Ticket?id={{ticket.id}}" method="post">
			<p >Editar Boleto</p>
			<input name="serial" type="text"  value="{{ticket.serial}}">
			<input name="eventName" type="text"  value="{{ticket.eventName}}">
			<input name="date" type="text"  readonly="readonly" value="{{ticket.date}}">
			<select  name="ubication">
				<option selected value="{{ticket.ubication}}">{{ticket.ubication}}</option>
				<option value="Altos">Altos</option>
				<option value="Medios">Medios</option>
				<option value="VIP">VIP</option>
				<option value="Platino">Platino</option>
			</select>
			<button type="submit" >Registrar</button>
		</form>
	</div>
{% endblock %}
